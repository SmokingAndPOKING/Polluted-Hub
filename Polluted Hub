-- âœ… PetSim/PetsGO Tab
local PetSimSection = PetSimTab:AddSection("Pet Simulator Scripts", 1)
AddConfirmableButton(PetSimSection, "Auto Hatch Eggs", "Automatically hatches eggs", "")
AddConfirmableButton(PetSimSection, "Pet Merger", "Auto merges pets for power", "")
AddConfirmableButton(PetSimSection, "Coin Magnet", "Auto collects coins around you", "")
for i = 1, 4 do AddConfirmableButton(PetSimSection, "COMING SOON", "More PetSim features soon!", "") end

-- ðŸ•’ Final Load Notification
local Time = (string.format("%."..tostring(Decimals).."f", os.clock() - Clock))
PlayNotificationSound()
library:SendNotification(("Loaded In "..tostring(Time)), 6)
